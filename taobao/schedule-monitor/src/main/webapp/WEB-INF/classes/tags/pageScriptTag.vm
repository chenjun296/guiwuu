<script type="text/javascript" src="$ctx/js/ajaxGrid.js"></script>
<div id='$id'></div>
<script  type="text/javascript">
var adPreview,tableUpdateSuccess,adSearch,tableUpdateError,previewError,
deleteSuccess,deleteError,newCell,before,after;
var table = {
	       title: '$title',
	      height: '$height',
	       width: '$width',
	        pKey: '$pKey',
	    editable: '$editable',
	     pageUrl: '$pageUrl',
	   deleteUrl: '$deleteUrl',
	   searchUrl: '$searchUrl',
	adSearchUrl: '$adSearchUrl',
	  previewUrl: '$previewUrl',
	      addUrl: '$addUrl',
	     editUrl: '$editUrl',
	   pageStyle: $pageStyle,
	     pageNum: $pageNum,
	previewStyle: '$previewStyle',
	        rows: $rows,
	        recycleUrl:'$recycleUrl',
	        cookie:$cookie
};
var column =$column;
var extendFunction = {
	adPreview: adPreview,
	tableUpdateSuccess: tableUpdateSuccess,
	adSearch: adSearch,
	tableUpdateError: tableUpdateError,
	previewError: previewError,
	deleteSuccess: deleteSuccess,
	deleteError: deleteError,
	newCell: newCell,
	before:before,
	after:after
};
jQuery('#$id').ajaxGrid({
	 tab: table,
	 col: column,
	exfn: extendFunction
});
</script>



