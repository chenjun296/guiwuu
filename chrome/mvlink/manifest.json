{
  "name": "mvlink",
  "version": "0.1",
  "description": "Move a link from one to another",
  "background_page": "extension.html",
  "options_page": "options.html",
  "icons": {
    "16": "icon.jpg",
    "48": "icon.jpg",
    "128": "icon.jpg"
  },
  "browser_action": {
    "default_icon": "icon.jpg"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*"
  ],
  "content_scripts": [ {
    "matches": ["http://*/*"], 
    "js": ["content.js"],
    "all_frames": true
  }]
}
